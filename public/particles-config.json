{
  "particles": {
    "number": {
      "value": 120,
      "density": {
        "enable": true,
        "value_area": 800
      }
    },
    "color": {
      "value": "ENC_COLOR_VALUE"
    },
    "shape": {
      "type": "circle",
      "stroke": {
        "width": 0,
        "color": "ENC_STROKE_COLOR"
      },
      "polygon": {
        "nb_sides": 5
      }
    },
    "opacity": {
      "value": 0.7,
      "random": true,
      "anim": {
        "enable": true,
        "speed": 0.5,
        "opacity_min": 0.3,
        "sync": false
      }
    },
    "size": {
      "value": 2.5,
      "random": true,
      "anim": {
        "enable": false,
        "speed": 20,
        "size_min": 0.1,
        "sync": false
      }
    },
    "line_linked": {
      "enable": true,
      "distance": 120,
      "color": "ENC_LINE_COLOR",
      "opacity": 0.5,
      "width": 1
    },
    "move": {
      "enable": true,
      "speed": 2,
      "direction": "none",
      "random": true,
      "straight": false,
      "out_mode": "out",
      "bounce": false,
      "attract": {
        "enable": false,
        "rotateX": 600,
        "rotateY": 1200
      }
    },
    "encryption": {
      "enable": true,
      "algorithm": "AES-256-GCM",
      "key": "ENC_KEY_PLACEHOLDER",
      "iv": "ENC_IV_PLACEHOLDER",
      "dataFields": ["color.value", "line_linked.color", "shape.stroke.color"],
      "obfuscate": true,
      "encryptedValuePrefix": "ENC_",
      "keyRotationInterval": 86400,
       "hmacEnable": true,
       "hmacAlgorithm": "SHA256",
      "salt": "ENC_SALT_PLACEHOLDER",
      "iterations": 10000,
      "base64Encode": true,
      "keyDerivationRounds": 3,
      "integrityCheck": true,
      "adata": "particles-config",
      "compression": {
        "enable": true,
        "algorithm": "zlib",
        "level": 9
      },
      "keyEncoding": "base64",
      "cacheEncryptedData": true,
      "adataEncoding": "base64",
      "randomizeIv": true,
      "hmacKey": "ENC_HMAC_KEY_PLACEHOLDER",
      "adataHmac": true,
      "debug": false,
      "performanceOptimization": {
        "disableEncryptionOnLowPerformance": true,
        "cpuUsageThreshold": 80,
        "memoryUsageThreshold": 90,
        "disableOnBattery": true,
        "maxConcurrentDecryptions": 4,
        "debounceTime": 100
      },
      "hmacSalt": "ENC_HMAC_SALT_PLACEHOLDER",
      "cacheTtl": 3600,
      "entropyEnhancement": true,
      "useHardwareAcceleration": true,
      "adataCompression": true,
      "nonceLength": 12,
      "tagLength": 16,
      "keySize": 32,
      "hmacKeySize": 32,
      "adataHmacKey": "ENC_ADATA_HMAC_KEY_PLACEHOLDER",
      "adataHmacAlgorithm": "SHA256",
      "encryptedNumberPrecision": 3,
      "enableWebAssembly": true,
      "webAssemblyThreads": 2,
      "entropySeed": "ENC_ENTROPY_SEED_PLACEHOLDER",
      "hmacIterationOffset": 10,
      "adataIterationOffset": 5,
       "parallelDecryption": true,
        "disableLogging": true,
        "adataSalt": "ENC_ADATA_SALT_PLACEHOLDER",
        "adataIterations": 7500,
        "hmacCompression": {
          "enable": true,
          "algorithm": "zlib",
          "level": 6
        },
        "adataHmacCompression": {
          "enable": true,
          "algorithm": "zlib",
          "level": 6
        },
         "deriveAdataFromKey": true,
          "dynamicKeyRotation": {
            "enable": true,
            "interval": 3600,
            "jitter": 0.1
          },
        "precomputeHashes": true,
        "hashCacheSize": 1000,
        "disableAdataEncryption": false,
        "streamingMode": true,
        "dataIntegrityAlgorithm": "SHA256",
        "adataCacheTtl": 7200,
        "hmacCacheTtl": 7200,
        "keyCacheTtl": 7200,
        "trustedOrigin": "self",
        "adataEncryptionKey": "ENC_ADATA_ENCRYPTION_KEY_PLACEHOLDER"
    },
    "wasm": {
        "url": "wasm/encryption.wasm",
        "fallbackUrl": "wasm/encryption.js"
    }
  },
  "interactivity": {
    "detect_on": "canvas",
    "events": {
      "onhover": {
        "enable": true,
        "mode": "repulse"
      },
      "onclick": {
        "enable": true,
        "mode": "push"
      },
      "resize": true
    },
    "modes": {
      "grab": {
        "distance": 100,
        "line_linked": {
          "opacity": 1
        }
      },
      "bubble": {
        "distance": 400,
        "size": 40,
        "duration": 2,
        "opacity": 0.8,
        "speed": 3
      },
      "repulse": {
        "distance": 150,
        "duration": 0.4
      },
      "push": {
        "particles_nb": 4
      },
      "remove": {
        "particles_nb": 2
      }
    }
  },
  "retina_detect": true,
  "config_demo": {
    "hide_card": false,
    "background_color": "#2c3e50",
    "background_image": "",
    "background_position": "50% 50%",
    "background_repeat": "no-repeat",
    "background_size": "cover"
  },
  "security": {
    "cors": {
      "enabled": true,
      "origin": "*"
    },
    "csp": {
      "enabled": true,
      "directives": {
        "default-src": "'self'",
        "script-src": "'self' 'unsafe-inline' 'unsafe-eval'",
        "style-src": "'self' 'unsafe-inline'",
        "img-src": "'self' data:",
        "connect-src": "'self'"
      }
    },
    "rateLimit": {
      "enabled": true,
      "windowMs": 60000,
      "max": 1000
    },
     "disableSourceMaps": true,
     "frameGuard": {
          "enabled": true,
          "policy": "DENY"
     },
     "subresourceIntegrity": {
       "enabled": true,
       "algorithm": "sha384"
     },
     "strictTransportSecurity": {
         "enabled": true,
         "maxAge": 31536000,
         "includeSubDomains": true,
         "preload": true
     },
     "xssProtection": {
         "enabled": true,
         "mode": "block"
     },
      "referrerPolicy": "strict-origin-when-cross-origin",
       "featurePolicy": {
           "enabled": true,
           "directives": {
               "geolocation": "'none'",
               "midi": "'none'",
               "notifications": "'none'",
               "camera": "'none'",
               "microphone": "'none'",
               "speaker": "'none'",
               "vibrate": "'none'",
               "payment": "'none'"
           }
       },
       "contentSecurityPolicyReportOnly": {
            "enabled": false,
            "reportUri": "/csp-report"
       },
        "dnsPrefetchControl": {
            "enabled": true,
            "policy": "on"
        },
        "expectCt": {
            "enabled": true,
            "maxAge": 86400,
            "enforce": true
        },
        "certificateTransparency": {
            "enabled": true,
            "enforce": true,
            "maxAge": 31536000,
            "reportUri": "/ct-report"
        }
  }
}