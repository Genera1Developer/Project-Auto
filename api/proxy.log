import logging

logging.basicConfig(level=logging.INFO, format='%(asctime)s - %(levelname)s - %(message)s')

def log_request(method, url, headers=None, data=None):
    log_message = f"Request: {method} {url}"
    if headers:
        log_debug(f"Request Headers: {headers}")
    if data:
        log_debug(f"Request Data: {data}")
    logging.info(log_message)

def log_response(status_code, url=None, headers=None, data=None):
    log_message = f"Response: Status Code {status_code}"
    if url:
        log_message += f" URL: {url}"
    if headers:
        log_debug(f"Response Headers: {headers}")
    if data:
        log_debug(f"Response Data: {data}")
    logging.info(log_message)

def log_error(message, exc_info=True):
    logging.error(message, exc_info=exc_info)

def log_debug(message):
    logging.debug(message)

if __name__ == '__main__':
    log_request("GET", "https://www.example.com", headers={"User-Agent": "Test"})
    log_response(200, url="https://www.example.com", data="Example data")
    log_error("An example error occurred")
    log_debug("Debugging message")