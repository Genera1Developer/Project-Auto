{
  "watch": ["*.js", "public", "uv", "api"],
  "ignore": ["node_modules", "cache/*", "storage/*", "uv/node_modules", "public/js/sw.js", "public/uv/uv.bundle.js", "public/uv/uv.config.js", "log/*", "public/img/*", "tmp/*"],
  "ext": "js,html,css,txt,json",
  "delay": "100",
  "legacyWatch": true,
  "debounceTimeout": 150,
  "verbose": true,
  "restartable": "rs",
  "execMap": {
    "js": "node"
  },
  "env": {
    "NODE_ENV": "development"
  }
}