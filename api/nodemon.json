{
  "watch": ["api", "public", "uv", "index.js", "config.js"],
  "ignore": [
    "node_modules/",
    "cache/*",
    "storage/*",
    "uv/node_modules/",
    "public/js/sw.js",
    "public/uv/uv.bundle.js",
    "public/uv/uv.config.js",
    "log/*",
    "tmp/*",
    "public/img/*",
    "cert/*",
    "db/*",
    ".cache/*",
    "sessions/*",
    "docs/*",
    "test/*"
  ],
  "ext": "js,json,html,css,mjs",
  "delay": "250",
  "legacyWatch": true,
  "debounceTimeout": 200,
  "verbose": true,
  "restartable": "rs",
  "execMap": {
    "js": "node"
  },
  "env": {
    "NODE_ENV": "development",
    "PORT": "3000",
    "DEBUG": "proxy:*",
    "UV_THREADPOOL_SIZE": "128"
  }
}