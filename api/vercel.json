{
  "version": 2,
  "builds": [
    {
      "src": "/api/proxy.js",
      "use": "@vercel/node",
      "config": { "maxDuration": 30 }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/proxy.js"
    }
  ],
  "functions": {
    "api/proxy.js": {
      "memory": 256,
      "maxDuration": 30
    }
  }
}