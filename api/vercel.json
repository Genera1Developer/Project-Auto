{
  "version": 2,
  "builds": [
    {
      "src": "/api/proxy.js",
      "use": "@vercel/node",
      "config": { "maxDuration": 60 }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/proxy.js"
    }
  ],
  "functions": {
    "api/proxy.js": {
      "memory": 512,
      "maxDuration": 60
    }
  },
  "github": {
    "silent": true
  }
}